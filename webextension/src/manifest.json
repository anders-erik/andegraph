{
	"description": "Provides full context andegraph graph-interaction. Primary purpose is sourcing and sharding. ",
	"manifest_version": 2,
	"name": "Andegraph",
	"version": "0.3.0",
	"homepage_url": "https://github.com/anders-erik/sources",
	"icons": {
		"48": "icons/andegraph-48.png"
	},
	"permissions": [
		"<all_urls>",
		"activeTab",
		"tabs",
		"clipboardRead",
		"clipboardWrite"
	],
	"browser_action": {
		"default_icon": "icons/andegraph-32.png",
		"theme_icons": [
			{
				"light": "icons/andegraph-32-light.png",
				"dark": "icons/andegraph-32.png",
				"size": 32
			}
		],
		"default_title": "sources",
		"default_popup": "popup/popup.html"
	},
	"web_accessible_resources": [
		"content_scripts/*",
		"popup/*"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content_scripts/ContentMessages.js",
				"overlay-dist/dist.js",
				"overlay-dist/wp-build.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background/background.js"
		]
	},
	"commands": {
		"activate_extension": {
			"suggested_key": {
				"default": "Alt+M"
			},
			"description": "Activate Your Extension"
		},
		"active": {
			"suggested_key": {
				"default": "Alt+A"
			},
			"description": "Toggle extension."
		}
	}
}