{
	"description": "Provides full context andegraph graph-interaction. Primary purpose is sourcing and sharding. ",
	"manifest_version": 2,
	"name": "Andegraph",
	"version": "1.0",
	"homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
	"icons": {
		"48": "icons/beasts-48.png"
	},
	"permissions": [
		"<all_urls>",
		"activeTab",
		"tabs",
		"clipboardRead",
		"clipboardWrite"
	],
	"browser_action": {
		"default_icon": "icons/beasts-32.png",
		"theme_icons": [
			{
				"light": "icons/beasts-32-light.png",
				"dark": "icons/beasts-32.png",
				"size": 32
			}
		],
		"default_title": "Beastify",
		"default_popup": "popup/choose_beast.html"
	},
	"web_accessible_resources": [
		"beasts/*.jpg",
		"content_scripts/*"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content_scripts/ContentMessages.js",
				"overlay-dist/dist.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background/back_functions.js",
			"background/back_messages.js",
			"background/back_events.js",
			"background/back_dbis-we.js",
			"background/back_main.js"
		]
	},
	"commands": {
		"activate_extension": {
			"suggested_key": {
				"default": "Alt+M"
			},
			"description": "Activate Your Extension"
		},
		"active": {
			"suggested_key": {
				"default": "Alt+A"
			},
			"description": "Start and stop shard capture"
		},
		"capture": {
			"suggested_key": {
				"default": "Alt+Q"
			},
			"description": "Start and stop shard capture"
		}
	}
}